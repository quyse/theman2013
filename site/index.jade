html
	head
		meta(http-equiv='content-type', content='text/html; charset=utf-8')
		title Тельняшка против тирании
		link(rel='stylesheet', type='text/css', href='<%styles.css%>')
		//- ___SCRIPTS___
		- if(scripts) for(var i in scripts)
			script(type='text/javascript', src=scripts[i])
	body
		canvas#canvas(width='600', height='600')
		div(style='left: 0; top:0; padding: 0; margin: 0; position:fixed; width:600px; height:20px; font-family: Arial; font-size:16px; color: red;')
			div#health(style='float:left') Health: 0
			div#score(style='float:right') Score: 0
		div#result(style='position:fixed; text-align:center; color:red; top:200px; left:0; width:600px; height:600px; font-family:Arial; font-size:64px;')
